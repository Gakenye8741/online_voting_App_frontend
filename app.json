{
  "expo": {
    "name": "Laikipia E-Vote",
    "slug": "LaikipiaE-Vote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Laikipia-logo.png",
    "scheme": "laikipiaevote",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gakenye.laikipiaevote"
    },
    "android": {
      "package": "com.gakenye.laikipiaevote",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Laikipia-logo.png",
        "backgroundColor": "#FFFFFF" 
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Laikipia-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Laikipia-logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a235228c-0bde-4439-a52c-b16326e1fb06"
      }
    }
  }
}